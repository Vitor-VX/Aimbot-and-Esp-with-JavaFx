<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>

<ScrollPane id="menuScrollPane" hbarPolicy="NEVER" prefHeight="310.0" prefWidth="220.0" stylesheets="css/estilo.css" vbarPolicy="NEVER" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.victor.vx.ControllerMenu">

    <content>
        <VBox id="menu" spacing="10">
            <Label id="titleLabel" text="Vitor-VX" />
            <Button fx:id="ativarFuncoes" onAction="#handleAtivarFuncoes" text="Ativar Funções" />
            <Button fx:id="aimbotAutomatic" onAction="#handleAimbotAutomatic" text="Aimbot Automatico" />
            <Button fx:id="aimbotFromShot" onAction="#handleAimbotFromShot" text="Aimbot por tiro" />
            <Button fx:id="balletInfity" onAction="#handleBalletInfity" text="Ballet Infinity" />
            <Button fx:id="norecoil" onAction="#handleNoRecoil" text="No Recoil" />
            <Button fx:id="espActivate" onAction="#handleEspAtivar" text="Ativar ESP" />
            <Button fx:id="espCima" onAction="#handleEspCima" text="ESP CIMA" />
            <Button fx:id="espBaixo" onAction="#handleEspBaixo" text="ESP BAIXO" />
        </VBox>
    </content>
</ScrollPane>
